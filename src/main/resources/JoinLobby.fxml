<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="1080.0" fitWidth="1920.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/background.jpg" />
         </image></ImageView>
       
      <HBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0">
          <ListView fx:id="lobbyList" prefHeight="1080.0" prefWidth="500.0" >
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </ListView>
      </HBox>
   </children>
</StackPane>
